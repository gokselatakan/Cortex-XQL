Device Name Search
dataset = xdr_data 
| filter agent_hostname = "DeviceName" 
| fields _time, dst_action_external_hostname, agent_hostname, actor_effective_username, action_local_ip, action_local_port, action_remote_ip, action_remote_port, actor_process_image_name, actor_process_command_line, actor_process_os_pid, actor_process_signature_status, agent_os_type
| view column order = populated
